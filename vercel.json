{
  "functions": {
    "api/forms.py": {
      "runtime": "python3.9"
    },
    "api/generate_question.py": {
      "runtime": "python3.9"
    },
    "api/submissions.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "outputDirectory": "frontend/build"
}